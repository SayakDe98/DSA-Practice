arr= {1,2,2,1,5,7,7}

cnt ={0,0,0,0,0,0,0}

i=0: cnt[1]++;
i = 1: cnt[2]++
i =2:cnt[2]++
i =3:cnt[1]++
i=4;cnt[5]++
i=5:cnt[7]++
i=6:cnt[7]++

cnt={0,2,1,0,0,1,0,2}

pos = 1

sum = 0
i=1:cnt[1] =2: sum += abs(1-1) :sum=0,cnt[1]=1,pos=2
i=2:cnt[2]=1:sum +=abs(2-1):sum=1,cnt[2]=0,pos=3
i=3:cnt[3]=0
i=4:cnt[4]=0
i=5:cnt[5]=1:sum+=abs(3-5);sum=3,pos=4
i=6:cnt[6]=0
i=7:cnt[7]=2:sum+=abs(4-7);sum=6,pos=5
